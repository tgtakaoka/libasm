# Ninja build script for libasm/test

root = ..
cxx      = g++
cxxflags = -std=c++14 -Wall -O ${debug_flags} -DLIBASM_DEBUG_VALUE
cppflags = -I${root}/src -I${root}/driver

rule cxx
  deps    = gcc
  depfile = ${out}.d
  command = ${cxx} ${cxxflags} ${cppflags} -MMD -MP -MF ${out}.d -c ${in} -o ${out}

rule link
  command = ${cxx} ${cxxflags} ${in} -o ${out}

rule run
  command = ${cxx} ${cxxflags} ${in} -o ${out} && ./${out}

subninja ${root}/driver/ninja.rules
subninja ${root}/src/ninja.rules
subninja ${root}/test/ninja.rules

# cdp1802
build test_asm_cdp1802: run test_asm_cdp1802.o $
  asm_cdp1802.o reg_cdp1802.o table_cdp1802.o text_cdp1802.o $
  asm_base.o config_base.o error_reporter.o insn_base.o operators.o $
  option_base.o parsers.o reg_base.o str_buffer.o str_scanner.o $
  text_common.o value.o value_parser.o $
  fixed64.o ieee_float.o float80_base.o float80_hard.o float80_soft.o $
  dec_float.o ibm_float.o test_asm_helper.o test_asserter.o
build test_dis_cdp1802: run test_dis_cdp1802.o $
  dis_cdp1802.o reg_cdp1802.o table_cdp1802.o text_cdp1802.o $
  dis_base.o config_base.o error_reporter.o formatters.o insn_base.o $
  option_base.o reg_base.o str_buffer.o str_scanner.o text_common.o value.o $
  value_formatter.o value_parser.o parsers.o operators.o dec_float.o $
  fixed64.o ieee_float.o float80_base.o float80_hard.o float80_soft.o $
  test_dis_helper.o test_asserter.o
build gen_cdp1802: link gen_cdp1802.o $
  dis_cdp1802.o reg_cdp1802.o table_cdp1802.o text_cdp1802.o $
  config_base.o dis_base.o dis_formatter.o error_reporter.o formatters.o $
  insn_base.o list_formatter.o option_base.o reg_base.o str_buffer.o $
  str_scanner.o test_generator.o text_common.o value.o value_formatter.o $
  fixed64.o ieee_float.o float80_base.o float80_hard.o float80_soft.o $
  dec_float.o value_parser.o parsers.o operators.o $
  gen_driver.o tokenized_text.o

